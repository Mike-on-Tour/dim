{
    "name": "mot/dim",
    "type": "phpbb-extension",
    "description": "This extension lets the administrator after a selectable number of days automatically delete any members who have neither activated their account nor visited the site in question after activation nor posted.",
    "homepage": "https://www.mike-on-tour.com",
    "version": "0.2.0",
    "time": "2024-04-10 12:11:57",
    "keywords": [
        "phpbb",
        "extension"
    ],
    "license": "GPL-2.0-only",
    "authors": [
		{
			"name": "Michael Vormwald (Mike-on-Tour)",
			"email": "admin@mike-on-tour.com",
			"homepage": "https://www.mike-on-tour.com",
			"role": "Ext Developer"
        }
    ],
    "require": {
		"php": ">=7.4, <8.4.0",
        "phpbb/phpbb": ">=3.3.1, <3.4.0@dev",
        "composer/installers": "~1.0.0"
    },
    "extra": {
        "display-name": "Delete Inactive Members",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.1, <3.4.0@dev"
        },
		"version-check": {
			"host": "www.mike-on-tour.com",
			"directory": "/mot",
			"filename": "mot_dim.json"
		}
    }
}
